   "source": [
    "#Write a python program to flatten tuple of list to tuple\n",
    "a=[[1,2,3],[4,5,6],[7],[8,9]]\n",
    "tup=[]\n",
    "for i in a:\n",
    "    for j in i:\n",
    "        tup.append(j)\n",
    "tup=tuple(tup)\n",
    "print(\"Original List:\",a,\"Type:\",type(a))\n",
    "print(\"Flattened Tuple:\",tup,\"Type:\",type(tup))"
   ]
  },
